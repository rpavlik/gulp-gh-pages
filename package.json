{
  "name": "@ryan.pavlik/gulp-gh-pages",
  "version": "1.0.4",
  "description": "Gulp v4 plugin to publish contents to Github pages",
  "repository": "rpavlik/gulp-gh-pages",
  "license": "MIT",
  "contributors": [
    "Github contributors <https://github.com/rpavlik/gulp-gh-pages/graphs/contributors>"
  ],
  "keywords": [
    "git",
    "push",
    "commit",
    "branch",
    "deploy",
    "deployment",
    "publish",
    "site",
    "website",
    "gulp",
    "gulpplugin",
    "gh-pages",
    "dist",
    "github"
  ],
  "files": [
    "index.js",
    "lib.js"
  ],
  "scripts": {
    "pretest": "eslint .",
    "test": "nyc --reporter=html --reporter=text mocha test.js --timeout 50000 --full-trace"
  },
  "dependencies": {
    "fancy-log": "^1.3.2",
    "finished": "^1.2.2",
    "gift": "^0.10.2",
    "inspect-with-kind": "^1.0.5",
    "plugin-error": "^1.0.1",
    "rimraf": "^2.6.2",
    "vinyl": "^2.2.0",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@justeat/eslint-config-fozzie": "^2.0.1",
    "@shinnn/eslint-config": "^6.8.1",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "load-gh-token": "^1.0.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "nyc-config-common": "^1.0.1",
    "octonode": "^0.9.5",
    "read-remove-file": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "@shinnn",
    "rules": {
      "no-underscore-dangle": "off"
    }
  },
  "nyc": {
    "extends": "nyc-config-common"
  }
}
